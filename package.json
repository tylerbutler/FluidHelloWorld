    "devDependencies": {
        "@snowpack/plugin-typescript": "^1.2.1",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/snowpack-env": "^2.3.3",
        "clean-webpack-plugin": "^3.0.0",
        "concurrently": "^5.3.0",
        "html-webpack-plugin": "^4.3.0",
        "snowpack": "^3.1.2",
        "tinylicious": "0.4.18879",
        "ts-loader": "^8.0.18",
